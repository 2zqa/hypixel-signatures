{% extends 'guild/members.twig' %}

{% block page_title %}{{ guild.name }} Bed Wars statistics{% endblock %}
{% block social_title %}{{ guild.name }} Bed Wars statistics - {{ config_get('app.name') }}{% endblock %}
{% block social_description %}View the Bed Wars statistics of the Hypixel guild {{ guild.name }} on {{ config_get('app.name') }}. You can browse through the list and compare Bed Wars statistics of guild members.{% endblock %}

{% block table_title %}<h1><a href="{{ route('guild.info', [guild.name]) }}">{{ guild.name }}</a>'s Bed Wars statistics</h1>{% endblock %}
{% block members_table %}
    <bed-wars-table :members="members"></bed-wars-table>
{% endblock %}
